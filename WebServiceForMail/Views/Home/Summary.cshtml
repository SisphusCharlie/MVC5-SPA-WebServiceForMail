@model  IEnumerable<WebServiceForMail.Models.MailInfo>

<div class="panel-heading">MailInfo Summary</div>
<div class="panel-body">
    <table class="table table-striped table-condensed">
        <thead>
            <tr><th>MailID</th><th>UserID</th><th>UserName</th><th>MailAddress</th><th>Goup</th><th>PhoneNum</th><th></th></tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.MailID</td>
                    <td>@item.UserID</td>
                    <td>@item.UserName</td>
                    <td>@item.MailAddress</td>
                    <td>@item.Goup</td>
                    <td>@item.PhoneNum</td>
                    <td>
                        @Html.ActionLink("Remove", "Remove",
                                new { id = item.MailID },
                                new { @class = "btn btn-xs btn-primary" })
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>


