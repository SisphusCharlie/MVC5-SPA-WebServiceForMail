@model WebServiceForMail.Models.MailInfo

<div class="panel-heading">
    Create MailInfos
</div>
<div class="panel-body">
    @using (Html.BeginForm("Add", "Home"))
    {
        <div class="form-group">
            <label>UserID </label>
            @Html.TextBoxFor(m => m.UserID, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <label>UserName</label>
            @Html.TextBoxFor(m => m.UserName, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <label>MailAddress</label>
            @Html.TextBoxFor(m => m.MailAddress, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <label>Goup</label>
            @Html.TextBoxFor(m => m.Goup, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <label>Goup</label>
            @Html.TextBoxFor(m => m.PhoneNum, new { @class = "form-control" })
        </div>

        <button type="submit" class="btn btn-primary">Save</button>
    }
</div>


